{
    "name": "argus",
    "version": "1.1.0",
    "private": true,
    "description": "GitHub App to watch for repository's workflows with tests and pin screenshots differences images to bot's comment of pull request.",
    "author": "Barsukov Nikita <nikita.s.barsukov@gmail.com>",
    "license": "ISC",
    "homepage": "https://github.com/Tinkoff/argus",
    "keywords": [
        "probot",
        "github",
        "probot-app",
        "cypress",
        "screenshot",
        "screenshots"
    ],
    "scripts": {
        "start:watch": "concurrently --kill-others \"tsc --watch\" \"wait-on ./lib/index.js && probot run ./lib/index.js\"",
        "start": "tsc && probot run ./lib/index.js",
        "prettier": "prettier '**/*.{svg,yml,js,ts,html,md,less,json}'",
        "build": "tsc"
    },
    "dependencies": {
        "adm-zip": "0.5.9",
        "probot": "12.2.8",
        "toml": "3.0.0"
    },
    "devDependencies": {
        "@octokit/types": "7.5.1",
        "@tinkoff/eslint-config": "1.36.1",
        "@tinkoff/eslint-config-angular": "1.36.1",
        "@tinkoff/prettier-config": "1.32.1",
        "@types/adm-zip": "0.5.0",
        "@types/node": "18.7.23",
        "concurrently": "7.4.0",
        "smee-client": "1.2.3",
        "typescript": "4.8.4",
        "wait-on": "6.0.0"
    },
    "engines": {
        "node": "16.x.x",
        "npm": ">= 7",
        "yarn": "Please use npm instead of yarn to install dependencies"
    }
}
